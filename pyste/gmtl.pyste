Vec = Template('gmtl::Vec', 'gmtl/Vec.h')
Vec3f = Vec('float 3')
rename(Vec3f, 'Vec3f')
set_policy(Vec3f.getData, return_internal_reference())

Quat = Template('gmtl::Quat', 'gmtl/Quat.h')
Quatf = Quat('float')
rename(Quatf, 'Quatf')
set_policy(Quatf.getData, return_internal_reference())

Point = Template('gmtl::Point', 'gmtl/Point.h')
Point3f = Point('float 3')
rename(Point3f, 'Point3f')
