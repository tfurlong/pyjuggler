Matrix = Template('gmtl::Matrix', 'gmtl/Matrix.h')
Matrix44f = Matrix('float 4 4')
Matrix44i = Matrix('int 4 4')
rename(Matrix44f, 'Matrix44f')
rename(Matrix44i, 'Matrix44i')
set_policy(Matrix44f.getData, return_internal_reference())
set_policy(Matrix44i.getData, return_internal_reference())

