         dpp_save_LIBS="$LIBS"
         AC_CHECK_LIB([$lib], [pthread_create],
            [AC_CHECK_HEADER([pthread.h],
               [ dpp_pthread_lib="-l${lib}" ; break ],
               [AC_MSG_WARN([*** POSIX threads will not be used (header file not found) ***])])])
         LIBS="$dpp_save_LIBS"
      done

      LIBS="$dpp_save_orig_LIBS"

      AC_LANG_RESTORE
   fi

