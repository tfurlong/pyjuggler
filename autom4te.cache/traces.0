         DPP_MAKE_IS_GNU_MAKE([$dpp_make_is_gmake],
            [GMAKE="$dpp_make_is_gmake"],
            [ AC_CHECK_PROG([dpp_gmake_is_gmake], [gmake], [gmake], [no], $4)
